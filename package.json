{
  "name": "vue-ai-sdk-chat",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc -b && vite build",
    "preview": "vite preview",
    "server": "tsx server/index.ts",
    "test": "vitest run",
    "test:watch": "vitest",
    "start": "concurrently -n \"frontend,backend\" -c \"blue,green\" \"npm run dev\" \"npm run server\""
  },
  "dependencies": {
    "@ai-sdk/anthropic": "^2.0.37",
    "@ai-sdk/openai": "^2.0.53",
    "@hono/node-server": "^1.19.5",
    "ai": "^6.0.0-beta.67",
    "dotenv": "^17.2.3",
    "highlight.js": "^11.11.1",
    "hono": "^4.10.2",
    "marked": "^16.4.1",
    "vue": "^3.5.22",
    "zod": "^3.25.76",
    "zod-to-json-schema": "^3.24.6"
  },
  "devDependencies": {
    "@types/marked": "^5.0.2",
    "@types/node": "^24.6.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.8.1",
    "concurrently": "^9.2.1",
    "happy-dom": "^20.0.8",
    "tsx": "^4.20.6",
    "typescript": "~5.9.3",
    "vite": "^7.1.7",
    "vitest": "^4.0.1",
    "vue-tsc": "^3.1.0"
  }
}
